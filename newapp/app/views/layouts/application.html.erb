<!DOCTYPE html>
<html>
<head>
  <title>Bookietest</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
  Signed in as <%= current_user.email %> | <%= link_to "Log out", session_path, :method => :delete %>
<% else %>
  <%= link_to 'Login', '/login' %> | <%= link_to 'Signup', '/session/new' %>
<% end %>



<%= yield %>

</body>
</html>
